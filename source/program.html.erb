                          <a href="speaker.html#<%= program.programid %>"><%= p.messagetitle ? p.messagetitle : "NO DATA" %></a><br />
                        <% end %>
                      <% end %>
                    </td>
                    <% end %>
                    <!-- english cafe -->
                    <td></td>
                  </tr>
                  <tr>
                    <td>12:00-<br>13:00</td>
                    <td colspan="9">
                      昼食・スカウティングセッション(A会場)
                    </td>
                  </tr>
                  <tr>
                    <td>13:00-<br>13:45</td>
