<div class="members-page">
  <div class="headback bluehead">
    <div class="container">
      <div class="headtopcontents members">
        <img src="assets/img/members/members-title.png" alt="">
      </div>
    </div>
  </div>
  <div class="section-view blue3">
    <div class="section-backview colorview1 blue1"></div>
    <div class="section-backview colorview2 blue2"></div>
    <div class="container">
      <div class="backwhite-inner">
        <%= partial "modules/ticketbar" %>
        <div class="row">
          <div class="col-md-12 box-section">
            <h1><%= data.members.title %></h1>
            <p><%= data.members.description %></p>
          </div>
          <div class="col-md-12 box-section">
            <% data.members.members.each_slice(2) do |member| %>
            <div class="col-md-12">
              <% member.each do |m|%>
              <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="row">
                  <div class="col-md-12">
                    <div class="col-md-4">
                      <img src="<%= m.avatar_url %>" alt="userprofileimg" class="userprofileimg">
                    </div>
                    <div class="col-md-8">
                      <h2><%= m.name %></h2>
                      <p><%= m.body %></p>
                    </div>
                  </div>
                </div>
              </div>
              <% end %>
            </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
